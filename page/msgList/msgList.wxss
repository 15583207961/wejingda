/* page/msgList/msgList.wxss */
.item{
  display: flex;
  align-items: center;
  position: relative;
  margin-top: 32rpx;
}
.item-headpic{
  width: 96rpx;
  height: 96rpx;
  border-radius: 48rpx;
  margin-left: 32rpx;
  margin-right: 24rpx;
}
.msg{
  font-size: 24rpx;
  margin-top: 10rpx;
  color: #333;
}
.time{
  position: absolute;
  top: 0rpx;
  right: 32rpx;
  font-size: 24rpx;
}