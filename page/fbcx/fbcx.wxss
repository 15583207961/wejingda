page{
  padding: 0;
  margin: 0;
  box-sizing: content-box;
  background-color: #f5f5f5;
}

/* 图标 */
.icon-top{
height: 570rpx;
width: 100vw;
background-image: url("https://singlestep.cn/wejinda/resource/img/bg_jwlogin_top.png");
background-size: 100%;
background-repeat: no-repeat;
position: absolute;
z-index: -1;
}
.backImage{
  width: 52rpx;
  height: 52rpx;
  object-fit: fill;
  margin-left: 22rpx;
  margin-right: 223rpx; 
}
.titleTop{
position: fixed;
width: 100vw;
top:0;
display: flex;
align-items: center;  
z-index: 100;
}
.login-headpic{
  position: absolute;
  width: 118rpx;
  height: 118rpx;
  left: 307rpx;
  top: 334rpx;
}
.content{
  position: absolute;
  top: 500rpx;
  margin-left: 32rpx;
}
.sno,.pwd{
  margin: 0 auto;
  width: 686rpx;
  height: 102rpx;
  background: #fff;
  border-radius: 46px;
  margin-bottom: 21rpx;
  border-radius: 16rpx;
  padding-left: 32rpx;
  box-sizing: border-box;
  color: #333333;
} 
.ckbox{
  margin-left: 80rpx;
  
} 
.goBtn{
  margin-top: 72rpx;
  color: #fff;
  background-color: #0099FF;
  width: 686rpx;
  height: 92rpx;
}

.protocol{
  display: flex;
  align-items: center;
  padding-left: 32rpx;
color: #333333;
font-size: 24rpx;
margin-top:32rpx;
}
.protocol-src{
  width: 32rpx;
  height: 32rpx;
  margin-right: 16rpx;
  object-fit: fill;
}
.protocol view{
  color: #0793F3;
}
.autoLogin{
  margin-top: 30rpx;
  display: flex;
  align-items: center;
  padding-left: 32rpx;
  font-size: 24rpx;
}
.hint-item{
  display: flex;
  align-items: center;
  font-size: 24rpx;
  font-weight: normal;
  color: rgb(129, 129, 129);
  margin-bottom: 16rpx;
  margin-top:32rpx;
  margin-left: 32rpx;
}
.hint-item::before{
  content: "";
  display: block;
  width: 0;
  height: 0;
  border: 6rpx solid rgb(129, 129, 129);
  border-radius: 10rpx;
  margin-right: 14rpx;
}


/* 详情页面 */
.contain{
  padding: 32rpx;
  padding-top: 248rpx;

}
.row-1{
  font-size: 52rpx;
  margin-left: 32rpx;
  margin-bottom: 44rpx;
  color: #333;
}
.row-2,.row-3{
  font: 40rpx;
  margin-left: 32rpx;
  margin-bottom: 40rpx;
color: #333;

}
.row-3{
  margin-bottom: 76rpx;
}
.profile-info-tips{
font-size: 24rpx;
color: #191919;
margin-left: 32rpx;
margin-bottom: 30rpx;
}
.info-contain{
  width: 686rpx;
  background-color: #fff;
  border-radius: 24rpx;
  padding: 32rpx;
  padding-bottom: 0;
  box-sizing: border-box;
}
.info-item{
  margin-bottom: 40rpx;
  font-size: 34rpx;
  font-weight: 550;
  color: #333333;
}
.info-item text{
  font-weight: 400;
  margin-left: 40rpx;
}
.reports{
  margin: 24rpx 0 30rpx 32rpx;
  font-size: 24rpx;
  font-weight: normal;
  color: #191919;
}
.process{
  width: 686rpx;
  height: 184rpx;
  display: flex;
  align-items: center;
  border-radius: 24rpx;
  background-color: #0A7AFF;
  color: #fff;
  font-size: 34rpx;
  box-sizing: border-box;
  padding-left: 32rpx;
  position: relative;
}
.arrow{
  width: 20rpx;
  height: 20rpx;
  border: 4rpx solid #fff;
  border-left:0 ;
  border-bottom:0 ;
  transform: rotate(45deg);
  position: absolute;
  right: 32rpx;
}

/* process */
.process-item{
  position: relative;
  width: 686rpx;
  height: 184rpx;
  border-radius: 24rpx;
  background-color: #fff;
  margin: 0 auto;
  margin-top: 32rpx;
  display: flex;
  align-items: center;
  padding: 32rpx;
  box-sizing: border-box;
  font-size: 34rpx;
  font-weight: normal;
  color: #191919;

}
.process-item text{
  position: absolute;
  right: 32rpx;
  color: #B2B2B2;
}
.done-icon{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 32rpx; 
}
.top-process{
  width: 100vw;
  background: #FFFFFF;
box-shadow: 0px 2px 4px 0px rgba(231, 231, 231, 0.5);
position: fixed;
top: 0;
z-index: 1;
}