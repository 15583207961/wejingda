.toolBar{
  position: fixed;
  top: 0;
  z-index: 10;
  width: 100%;
  height: 112rpx;
  background-color: #fff;
  display: flex;
  font-size: 32rpx;
  align-items: center;
  box-shadow: 0px 4rpx 8rpx 0px rgba(231, 231, 231, 0.5);
}
.add-swzl{
  width: 96rpx;
  height: 96rpx;
  object-fit: fill;
  margin-left: 32rpx;
}
.lost{
 margin-left: 32rpx;
}
.pick{
  margin-left: 40rpx;
}
.select-color{
  color: #086BFF;
}
.swzl-sreach{
  width:342rpx;
  height: 72rpx;
  border-radius: 24rpx;
  margin-left: 56rpx;
