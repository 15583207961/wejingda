/* 空教室 */

.KJS_item{
    width: 95%;
    margin:  0 auto;
    height: 120rpx;
    background-color: rgb(255, 255, 255);
    border-radius: 24rpx;
    position: relative;
    line-height: 120rpx;
    font-size: 38rpx;
    font-weight: 600;
    color: green;
    padding-left: 20rpx;
    box-sizing: border-box;
    margin-bottom: 21rpx;
}
.number{
    position: absolute;
    top: 30rpx;
    right: 20rpx;
    font-size: 28rpx;
    font-weight: 500;
}
.number text{
    color: #797979;
}