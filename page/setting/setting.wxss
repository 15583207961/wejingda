page{
    background-color: #f5f5f5;
}

.setting-item{
    width: 686rpx;
    height: 108rpx;
    background-color: #fff;
    position: relative;
    display: flex;
    align-items: center;
    padding: 32rpx;
    box-sizing: border-box;
   
}
.clear,.about{
    border-radius: 16rpx;
    margin-top: 24rpx;
    margin-left: 32rpx;
}
.quit-login{
    display: inline-block;
    margin-top: 24rpx;
    margin-left: 32rpx;

    border-radius: 16rpx;
}
.setting-item::after{
    position: absolute;
    right: 32rpx;
    top: 44rpx;
    content: "";
    display: block;
    width: 20rpx;
    height: 20rpx;
    border: 4rpx rgb(178,178,178) solid;
    border-radius: 5rpx  ;
    border-left: 0;
    border-bottom: 0;
    transform: rotate(45deg);
}